    <head>
        <meta charset="UTF-8" />
        <!-- <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">  -->
        <title>Login</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description"
              content="Login and Registration Form with HTML5 and CSS3" />
        <meta name="keywords"
              content="html5, css3, form, switch, animation, :target, pseudo-class" />
        <meta name="author" content="Codrops" />
        <script
        src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
        <link rel="shortcut icon" href="../favicon.ico">
        <link rel="stylesheet" type="text/css" href="css/demo.css" />
        <link rel="stylesheet" type="text/css" href="css/style.css" />
        <link rel="stylesheet" type="text/css" href="css/animate-custom.css" />
        <script src="js/script.js"></script>
        <script src="js/service.js"></script>
    </head>

    <body ng-app="myApp"  ng-controller="appCtrl">
        <div ng-if="view == 'login'" ng-include="'login.html'"></div>
        <div ng-if="view == 'register'" ng-include="'register.html'"></div>
        <div ng-if="view == 'admindashboard'"  ng-include="'admindashboard.html'"></div>
        <div ng-if="view == 'quizName'"  ng-include="'quizName.html'" ></div>
        <div  ng-if="view == 'selectQuiz'"  ng-include="'selectQuiz.html'" > </div>
        <div ng-if="view == 'addquiz2'"  ng-include="'addquiz2.html'" ></div>
        <div ng-if="view == 'quizRules'"  ng-include="'quizRules.html'" ></div>
        <div ng-if="view == 'writequiz2'"  ng-include="'writequiz2.html'" ></div>
        <div ng-if="view == 'deleteQuiz'" ng-include="'deleteQuiz.html'"></div>
        <div ng-if="view == 'score'" ng-include="'score.html'"></div>
        <div ng-if="view == 'viewResultsFor'" ng-include="'viewResultsFor.html'"></div>
        <div ng-if="view == 'results'" ng-include="'results.html'"></div>
        <div ng-if="view == 'users'" ng-include="'users.html'"></div>
    </body>
</html>