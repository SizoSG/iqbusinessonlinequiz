<ng-form name="quizNameForm">
    <div class="container">
        <header>
            <h1>Oline<span>Quiz Rules</span></h1>
            <nav class="codrops-demos">
                <br>
                </br>
                
                <div class="form-group ">

 
                
           <div class="card mb-3">
        <div class="card-header">
          <i class="fa fa-table"></i><strong> List of Users</strong></div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          
               
                  
                  <th><strong>Name</strong></th>
                  <th><strong>Surname</strong></th> 
                   <th><strong>Email Address</strong></th>
                  <th><strong>Employee Number</strong></th>
                 
                 
              
                  
             <tr ng-repeat="x in userlist">
              <td>{{ x.name }}</td>
              <td>{{ x.surname }}</td>
              <td>{{ x.email }}</td>
              <td>{{ x.employeenumber }}</td>
              
              
            </tr>
                        
          
</table>
          </div>
        </div>
        <div class="card-footer small text-muted">Updated</div>
      </div>
    </div>
                
                
                
            </nav> 
        </header>
    </div>
    <div style="overflow:auto;">
        <div style="float:right;">
            <button type="button" ng-click="showPage('admindashboard');">Back</button>
        </div>
    </div>
<script src="vendor/jquery/jquery.min.js"></script>
